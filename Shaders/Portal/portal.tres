code = "shader_type spatial;
render_mode blend_mix, depth_draw_opaque, cull_disabled, diffuse_lambert, specular_schlick_ggx, unshaded;

uniform float color_intensity = 7;
uniform vec4 portal_color : source_color = vec4(0.000000, 0.349020, 1.000000, 1.000000);
uniform vec2 speed = vec2(-0.300000, 0.300000);
uniform sampler2D portal_texture : source_color;
uniform sampler2D portal_shape_texture : source_color;



void fragment() {
// FloatParameter:10
	float n_out10p0 = color_intensity;


// ColorParameter:8
	vec4 n_out8p0 = portal_color;


// VectorOp:11
	vec4 n_out11p0 = vec4(n_out10p0) * n_out8p0;


	vec2 n_out3p0;
// UVPolarCoord:3
	vec2 n_in3p1 = vec2(0.50000, 0.50000);
	float n_in3p2 = 1.00000;
	float n_in3p3 = 1.00000;
	{
		vec2 __dir = UV - n_in3p1;
		float __radius = length(__dir) * 2.0;
		float __angle = atan(__dir.y, __dir.x) * 1.0 / (PI * 2.0);
		n_out3p0 = vec2(__radius * n_in3p2, __angle * n_in3p3);
	}


// Input:5
	float n_out5p0 = TIME;


// Vector2Parameter:12
	vec2 n_out12p0 = speed;


// VectorOp:6
	vec2 n_out6p0 = vec2(n_out5p0) * n_out12p0;


// UVFunc:4
	vec2 n_in4p1 = vec2(1.00000, 1.00000);
	vec2 n_out4p0 = n_out6p0 * n_in4p1 + n_out3p0;


	vec4 n_out14p0;
// Texture2D:14
	n_out14p0 = texture(portal_texture, n_out4p0);
	float n_out14p1 = n_out14p0.r;


// VectorOp:9
	vec4 n_out9p0 = n_out11p0 * vec4(n_out14p1);


	vec4 n_out16p0;
// Texture2D:16
	n_out16p0 = texture(portal_shape_texture, UV);
	float n_out16p1 = n_out16p0.r;


// Output:0
	ALBEDO = vec3(n_out9p0.xyz);
	ALPHA = n_out16p1;


}
